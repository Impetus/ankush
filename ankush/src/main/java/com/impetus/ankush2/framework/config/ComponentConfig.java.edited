	/**
	 * Adds the advance conf property.
	 *
	 * @param key the key
	 * @param value the value
	 */
	public void addAdvanceConfProperty(String key, Object value) {
		if (this.advanceConf == null) {
			this.advanceConf = new LinkedHashMap<String, Object>();
		}
		this.advanceConf.put(key, value);
	}

	/**
	 * Gets the advance conf property.
	 *
	 * @param key the key
	 * @return the advance conf property
	 */
	public Object getAdvanceConfProperty(String key) {
		if (this.advanceConf == null || key == null) {
			return null;
		}
		return this.advanceConf.get(key);
	}
